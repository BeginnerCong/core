{
	"name": "openssl",
	"res_folder": "./deploy",
	"wasm": true,
	"asm": false,
	"run_before": "before.py",
	"run_after": "after.py",
	"base_js_content": "./engine.js",

	"compiler_flags": [
		"-O3",
		"-fno-rtti",
		"-Wno-unused-command-line-argument",
		"-s ALLOW_MEMORY_GROWTH=1",
		"-s FILESYSTEM=0",
		"-s ENVIRONMENT='web'"
	],
	"exported_functions": [
		"_malloc",
		"_free",
		"_Crypto_CreateCertificate",
		"_Crypto_LoadCert",
		"_Crypto_LoadKey",
		"_Crypto_DestroyCertificate",
		"_Crypto_CreateKeys"
	],
	"include_path": [
		"openssl/include", "openssl", "emsdk/node/14.15.5_64bit/include/node/openssl/archs/linux-x86_64/no-asm/include"
	],
	"define": [
		"__linux__", "_LINUX"
	],
	"compile_files_array": [
	{
		"name": "k",
		"folder": "../../../../common/",
		"files": ["Base64.cpp", "File.cpp"]
	},
	{
		"name": "s",
		"folder": "../../../../xmlsec/src/src/",
		"files": ["CertificateCommon.cpp"]
	},
	{
		"name": "a",
		"folder": "./",
		"files": ["openssl/apps/openssl.c", "./main.cpp"]
	}
	],
	"sources": ["./openssl/libcrypto.a"]
}
