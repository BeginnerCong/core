{
	"name": "hyphen",
	"res_folder": "../deploy",
	"wasm": true,
	"asm": true,
	"embed_mem_file": true,
	"run_before": "",
	"run_after": "",
	"base_js_content": "../module.js",

	"compiler_flags": [
		"-O3",
		"-sASSERTIONS",
		"-fno-rtti",
		"-Wno-unused-command-line-argument",
		"-sALLOW_MEMORY_GROWTH",
		"-sEXPORTED_RUNTIME_METHODS='stringToUTF8','UTF8ToString'"
	],
	"exported_functions": [
		"_malloc",
		"_free",
        "_hyphenCreateApplication",
        "_hyphenDestroyApplication",
		"_hyphenLoadDictionary",
		"_hyphenWord"
	],
	"include_path": ["../src"],
	"define": [],
	"compile_files_array": [
	{
		"name": "s",
		"folder": "../src",
		"files": ["ExportedFunctions.cpp", "HyphenApplication.cpp", "../../hyphen/hnjalloc.c"]
	}
	]
}
